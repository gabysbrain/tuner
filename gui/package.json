{
  "name": "tuner",
  "version": "0.0.0",
  "engines": {
    "node": ">=0.10.22"
  },
  "scripts": {
    "postinstall": "bower install",
    "test": "mocha --opts mocha/mocha.opts app/scripts/**/__tests__/*-test.* app/scripts/components/webgl/__tests__/*-test.js",
    "prebuild": "mkdir -p build",
    "build": "npm run build:css && npm run build:js && npm run build:html",
    "build:css": "sass app/scss/app.scss build/app.css",
    "watch:css": "watch 'npm run build:css' app/scss",
    "build:js": "npm run build:js:project_viewer && npm run build:js:shell",
    "build:js:project_viewer": "browserify --debug -t reactify app/scripts/app.js -o build/main.js",
    "build:js:shell": "cp app/scripts/shell.js build/shell.js",
    "watch:js": "watch 'npm run build:js' app/scripts",
    "build:html": "cp app/index.html build/index.html",
    "watch:html": "watch 'npm run build:html' app/",
    "watch": "parallelshell 'npm run watch:js' 'npm run watch:css' 'npm run watch:html'"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "browserify": "^5.12.1",
    "browserify-shim": "^3.7.0",
    "chai": "*",
    "connect": "^2.25.9",
    "connect-livereload": "^0.4.0",
    "debowerify": "^0.8.1",
    "istanbul": "^0.3.5",
    "jsdom": "*",
    "jshint-stylish": "^0.4.0",
    "mocha": "*",
    "node-sass": "^2.0.0-beta",
    "parallelshell": "*",
    "partialify": "^3.1.1",
    "react-tools": "^0.12.1",
    "reactify": "^0.17.1",
    "rosie": "*",
    "sinon": "*",
    "watch": "^0.13.0"
  },
  "dependencies": {
    "atom-shell": "^1.2.0",
    "cortexjs": "^0.6.2",
    "jquery": "^2.1.1",
    "jquery-ui": "^1.10.5",
    "react": "^0.12.1"
  }
}
