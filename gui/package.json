{
  "name": "gui",
  "version": "0.0.0",
  "engines": {
    "node": ">=0.10.22"
  },
  "scripts": {
    "postinstall": "bower install",
    "test": "mocha --opts mocha/mocha.opts app/scripts/**/__tests__/*-test.*",
    "prebuild": "mkdir -p build",
    "build": "npm run build:js && npm run build:html",
    "build:js": "browserify --debug -t reactify app/scripts/app.js -o build/main.js",
    "build:html": "cp app/index.html build/index.html"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "browserify": "^5.12.1",
    "browserify-shim": "^3.7.0",
    "chai": "*",
    "connect": "^2.25.9",
    "connect-livereload": "^0.4.0",
    "debowerify": "^0.8.1",
    "istanbul": "^0.3.5",
    "jsdom": "*",
    "jshint-stylish": "^0.4.0",
    "mocha": "*",
    "partialify": "^3.1.1",
    "react-tools": "^0.12.1",
    "reactify": "^0.17.1",
    "rosie": "*",
    "sinon": "*"
  },
  "dependencies": {
    "atom-shell": "^1.2.0",
    "cortexjs": "^0.6.2",
    "jquery": "^2.1.1",
    "jquery-ui": "^1.10.5",
    "react": "^0.12.1"
  }
}
